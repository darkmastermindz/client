{
  "protocol": "phoneNumbers",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "RawPhoneNumber",
      "fields": [],
      "typedef": "string"
    },
    {
      "type": "record",
      "name": "UserPhoneNumber",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone_number"
        },
        {
          "type": "boolean",
          "name": "verified",
          "jsonkey": "verified"
        },
        {
          "type": "IdentityVisibility",
          "name": "visibility",
          "jsonkey": "visibility"
        },
        {
          "type": "UnixTime",
          "name": "ctime",
          "jsonkey": "ctime"
        }
      ],
      "doc": "Phone number support for TOFU chats."
    },
    {
      "type": "record",
      "name": "PhoneNumberLookupResult",
      "fields": [
        {
          "type": "RawPhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone_number"
        },
        {
          "type": "PhoneNumber",
          "name": "coercedPhoneNumber",
          "jsonkey": "coerced_phone_number"
        },
        {
          "type": [
            null,
            "string"
          ],
          "name": "err",
          "jsonkey": "err"
        },
        {
          "type": [
            null,
            "UID"
          ],
          "name": "uid",
          "jsonkey": "uid"
        }
      ]
    },
    {
      "type": "record",
      "name": "PhoneNumberAddedMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    },
    {
      "type": "record",
      "name": "PhoneNumberVerifiedMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    },
    {
      "type": "record",
      "name": "PhoneNumberSupersededMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    },
    {
      "type": "record",
      "name": "ContactComponent",
      "fields": [
        {
          "type": [
            null,
            "RawPhoneNumber"
          ],
          "name": "phoneNumber"
        },
        {
          "type": [
            null,
            "EmailAddress"
          ],
          "name": "email"
        },
        {
          "type": "string",
          "name": "label"
        }
      ]
    },
    {
      "type": "record",
      "name": "Contact",
      "fields": [
        {
          "type": "string",
          "name": "name"
        },
        {
          "type": {
            "type": "array",
            "items": "ContactComponent"
          },
          "name": "components"
        }
      ]
    },
    {
      "type": "record",
      "name": "ResolvedContact",
      "fields": [
        {
          "type": "string",
          "name": "name"
        },
        {
          "type": "ContactComponent",
          "name": "component"
        },
        {
          "type": [
            null,
            "string"
          ],
          "name": "err"
        },
        {
          "type": "boolean",
          "name": "resolved"
        },
        {
          "type": "UID",
          "name": "uid"
        },
        {
          "type": "string",
          "name": "username"
        },
        {
          "type": "string",
          "name": "fullName"
        }
      ]
    }
  ],
  "messages": {
    "addPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "editPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "oldPhoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "verifyPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "code",
          "type": "string"
        }
      ],
      "response": null
    },
    "getPhoneNumbers": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        }
      ],
      "response": {
        "type": "array",
        "items": "UserPhoneNumber"
      }
    },
    "deletePhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        }
      ],
      "response": null
    },
    "setVisibilityPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "setVisibilityAllPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "bulkLookupPhoneNumbers": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumberContacts",
          "type": {
            "type": "array",
            "items": "RawPhoneNumber"
          }
        },
        {
          "name": "regionCodes",
          "type": {
            "type": "array",
            "items": "RegionCode"
          }
        },
        {
          "name": "userRegionCode",
          "type": [
            null,
            "RegionCode"
          ]
        }
      ],
      "response": {
        "type": "array",
        "items": "PhoneNumberLookupResult"
      }
    },
    "lookupContactList": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "contacts",
          "type": {
            "type": "array",
            "items": "Contact"
          }
        },
        {
          "name": "userRegionCode",
          "type": "RegionCode"
        }
      ],
      "response": {
        "type": "array",
        "items": "ResolvedContact"
      }
    }
  },
  "namespace": "keybase.1"
}